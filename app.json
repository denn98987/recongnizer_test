{
  "expo": {
    "name": "Recognizer",
    "slug": "Recognizer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/main-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.labcode.recognizertest",
      "buildNumber": "1.1.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA"
      ],
      "package": "com.labcode.recognizertest"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
